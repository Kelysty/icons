{
  "name": "@kelysty/icons",
  "version": "1.0.0",
  "description": "Pack of Kelysty React UI svg icons",
  "license": "MIT",
  "author": "Kelysty",
  "contributors": [
    "Ivan Postarnak"
  ],
  "keywords": [
    "icons",
    "svg",
    "react"
  ],
  "module": "esm/index.js",
  "sideEffects": false,
  "files": [
    "svgs",
    "esm",
    "*.d.ts",
    "*.js",
    "!.*.js",
    "!*.config.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kelysty/icons.git"
  },
  "bugs": {
    "url": "https://github.com/Kelysty/icons.git.git/issues"
  },
  "homepage": "https://github.com/Kelysty/icons.git.git#readme",
  "scripts": {
    "prepare": "husky install",
    "lint": "npm run lint:js && npm run lint:styles",
    "lint:js": "eslint --ext .js,.jsx,.mjs,.ts,.tsx ./lib/",
    "lint:styles": "stylelint '**/*.scss'",
    "prettier": "prettier '**/*.{js,md,yaml,yml,json}' --config .prettierrc.js",
    "build": "yarn run build:esm & yarn run build:cjs",
    "build:cjs": "tsc --declaration --outDir .",
    "build:esm": "tsc --module esnext --outDir esm",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.23.3",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@kelysty/eslint-config": "^1.0.0",
    "@kelysty/prettier-config": "^1.0.0",
    "@kelysty/stylelint-config": "^1.0.1",
    "@kelysty/tsconfig": "^1.0.0",
    "@types/react": "^18.2.38",
    "@types/react-dom": "^18.2.17",
    "eslint": "^8.54.0",
    "husky": "^8.0.3",
    "nano-staged": "^0.8.0",
    "postcss": "^8.4.31",
    "prettier": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "stylelint": "^15.11.0",
    "typescript": "^5.3.2"
  },
  "nano-staged": {
    "*.{scss}": [
      "stylelint --fix --quiet"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix --quiet"
    ],
    "*.md": [
      "prettier --write"
    ]
  }
}
